{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport TodoInput from './TodoInput.vue';\nimport TodoList from './TodoList.vue';\nexport default {\n  components: {\n    TodoInput,\n    TodoList\n  },\n  data: function () {\n    return {\n      todo: [{\n        id: 1,\n        text: '뷰 공부하기',\n        checked: false\n      }, {\n        id: 2,\n        text: '넉스트 알아보기',\n        checked: false\n      }]\n    };\n  },\n  methods: {\n    addTodo(text) {\n      if (this.todo.find(item => item.text === text) !== undefined) {\n        return alert('이미 목록에 있습니다.');\n      }\n      this.todo.push({\n        id: this.todo.length + 1,\n        text: text,\n        checked: false\n      });\n    },\n    deleteTodo: function (id) {\n      this.todo = this.todo.filter(item => item.id !== id);\n    },\n    checkedTodo: function (id) {\n      const todoList = [...this.todo];\n      const todoItem = todoList.find(item => item.id === id);\n      if (todoItem) {\n        todoItem.checked = !todoItem.checked;\n        this.todo = todoList;\n      }\n    }\n  },\n  computed: {\n    checkedText: function () {\n      return this.todo.checked ? 'finishText' : null;\n    }\n  }\n};","map":{"version":3,"mappings":";AASA,OAAOA,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,QAAO,MAAO,gBAAgB;AAErC,eAAe;EACbC,UAAU,EAAE;IACVF,SAAS;IACTC;EACF,CAAC;EACDE,IAAI,EAAE,YAAY;IAChB,OAAO;MACLC,IAAI,EAAE,CACJ;QACEC,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,QAAQ;QACdC,OAAO,EAAE;MACX,CAAC,EACD;QACEF,EAAE,EAAE,CAAC;QACLC,IAAI,EAAE,UAAU;QAChBC,OAAO,EAAE;MACX,CAAC;IAEL;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,OAAO,CAACH,IAAI,EAAE;MACZ,IAAI,IAAI,CAACF,IAAI,CAACM,IAAI,CAACC,IAAG,IAAKA,IAAI,CAACL,IAAG,KAAMA,IAAI,MAAMM,SAAS,EAAE;QAC5D,OAAOC,KAAK,CAAC,cAAc;MAC7B;MACA,IAAI,CAACT,IAAI,CAACU,IAAI,CAAC;QACbT,EAAE,EAAE,IAAI,CAACD,IAAI,CAACW,MAAK,GAAI,CAAC;QACxBT,IAAI,EAAEA,IAAI;QACVC,OAAO,EAAE;MACX,CAAC;IACH,CAAC;IACDS,UAAU,EAAE,UAAUX,EAAE,EAAE;MACxB,IAAI,CAACD,IAAG,GAAI,IAAI,CAACA,IAAI,CAACa,MAAM,CAACN,IAAG,IAAKA,IAAI,CAACN,EAAC,KAAMA,EAA